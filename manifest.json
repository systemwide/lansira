{
	"name": "Lansira Tab Manager",
	"description": "Easily manage and launch tab sets from the menu bar",
	"version": "0.3",
	

	"background": {
		"scripts": ["js/background.js"]
	},

	"browser_action": {
		"default_icon": "images/launch.png",
		"default_popup": "popup.html",
		"default_title": "Lansira Tab Manager"
	},
	
	"optional_permissions": [
		"tabs"
	],

	"permissions": [ 
		"notifications", 
		"tabs",
		"storage" ],

	"manifest_version": 2
}